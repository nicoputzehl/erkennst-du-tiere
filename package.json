{
	"name": "erkennst-du-das-tier",
	"main": "expo-router/entry",
	"version": "1.0.0",
	"scripts": {
		"start": "expo start",
		"start:logging": "EXPO_PUBLIC_LOGGING_ENABLED=true expo start",
		"reset-project": "node ./scripts/reset-project.js",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"lint": "expo lint",
		"test": "jest --watchAll",
		"ios:prebuild": "npx expo prebuild -p ios --clean",
		"cleanup": "rm -rf node_modules && rm -rf package-lock.json && npm i"
	},
	"dependencies": {
		"@expo-google-fonts/ubuntu": "^0.4.0",
		"@expo/ui": "~0.2.0-beta.7",
		"@expo/vector-icons": "^15.0.2",
		"@react-native-async-storage/async-storage": "2.2.0",
		"@react-navigation/bottom-tabs": "^7.3.10",
		"@react-navigation/elements": "^2.3.8",
		"@react-navigation/native": "^7.1.6",
		"expo": "^54.0.13",
		"expo-blur": "~15.0.7",
		"expo-constants": "~18.0.9",
		"expo-font": "~14.0.9",
		"expo-haptics": "~15.0.7",
		"expo-image": "~3.0.9",
		"expo-linear-gradient": "~15.0.7",
		"expo-linking": "~8.0.8",
		"expo-router": "~6.0.12",
		"expo-splash-screen": "~31.0.10",
		"expo-status-bar": "~3.0.8",
		"expo-symbols": "~1.0.7",
		"expo-system-ui": "~6.0.7",
		"expo-web-browser": "~15.0.8",
		"lottie-react-native": "~7.3.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-native": "0.81.4",
		"react-native-gesture-handler": "~2.28.0",
		"react-native-reanimated": "~4.1.1",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-web": "^0.21.0",
		"react-native-webview": "13.15.0",
		"react-native-worklets": "0.5.1",
		"react-test-renderer": "^19.1.0",
		"zustand": "^5.0.5"
	},
	"devDependencies": {
		"@babel/core": "^7.25.2",
		"@biomejs/biome": "1.9.4",
		"@redux-devtools/extension": "^3.3.0",
		"@testing-library/react-native": "13.2.0",
		"@types/jest": "^29.5.14",
		"@types/react": "~19.1.10",
		"eslint": "^9.25.0",
		"eslint-config-expo": "~10.0.0",
		"jest": "~29.7.0",
		"jest-expo": "~54.0.2",
		"ts-jest": "^29.3.4",
		"typescript": "~5.9.2"
	},
	"private": true,
	"jest": {
		"preset": "jest-expo",
		"transformIgnorePatterns": [
			"node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
		],
		"setupFilesAfterEnv": ["<rootDir>/jest-setup.ts"],
		"moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
		"testMatch": [
			"**/__tests__/**/*.(ts|tsx|js)",
			"**/*.(test|spec).(ts|tsx|js)"
		],
		"collectCoverageFrom": [
			"**/*.{ts,tsx}",
			"!**/coverage/**",
			"!**/node_modules/**",
			"!**/babel.config.js",
			"!**/jest.setup.ts"
		]
	},
	"packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
